<table>
  <tr>
    <th>Name</th>
    <th>Duration</th>
    <th>Edit</th>
    <th>Delete</th>
  </tr>
  <% @songs.each do |song| %>
    <tr>
      <td><%= song.name %></td>
      <td><%= song.duration %></td>
      <td><%= link_to "X", edit_song_path(song.id) %></td>
      <td><%= link_to "X", delete_song_path(song.id), method: 'delete' %></td>  
    </tr>
  <% end %>
</table>