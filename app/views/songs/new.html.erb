<%= form_for @song, method: 'post' do |f|  %>

  <%= render "shared/form_errors", model: @song %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :duration %>
  <%= f.number_field :duration %>
  <%= f.label :artist_id %>
  <%= f.select :artist_id, @artists %>
  <%= f.submit "Submit me bro" %>
<% end %>